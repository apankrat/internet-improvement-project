{
 "name": "Is A Follower",
 "description": "Adds an indicator to Dribbble user's profile and shots pages to show if he/she is folowing you",
 "homepage_url": "http://swapped.cc/iip",
 "update_url": "https://raw.github.com/apankrat/internet-improvement-project/master/dribbble/is-a-follower/chrome/update.xml",
 "permissions": [
     "tabs",
     "http://*.dribbble.com/*"
 ],
 "content_scripts": [
   {
     "matches": ["http://*.dribbble.com/*"],
     "js":      ["jquery-1.5.2.min.js", "jquery.tipsy-1.0.0a.js", "bookmarklet.js"]
   }
 ],
 "version": "2.1"
}